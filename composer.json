{
    "name": "mvdnbrk/nova-warehouse-scan",
    "description": "Laravel Nova Warehouse Scan Tool",
    "keywords": [
        "laravel",
        "nova",
        "warehouse",
        "scan"
    ],
    "homepage": "https://github.com/mvdnbrk/nova-warehouse-scan",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "laravel/nova": "^3.0",
        "mvdnbrk/warehouse-framework": "^0.16"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "orchestra/testbench": "^5.10 || ^6.5",
        "phpunit/phpunit": "^9.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Just\\Warehouse\\Nova\\Scan\\ToolServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Just\\Warehouse\\Nova\\Scan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Just\\Warehouse\\Nova\\Scan\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
